const PM = 44
const DS = 176
const dsize = 176 / 44

for (rseed = 0; rseed >= 0; rseed += 1) {
    print(rseed)
    printflush(message1)

    for (y = 0; y < dsize; y += 1) {
        for (x = 0; x < dsize; x += 1) {
            op(noise, val, x, y + rseed)
            op(abs, val, val)
            val *= 255
            draw(color, val, val, val, 255)
            draw(rect, x * PM, y * PM, PM, PM)
        }
    }
    drawflush(display1)
}
