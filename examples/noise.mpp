const PM = 44
const DS = 176
dsize = DS / PM

for (rseed = 0; rseed >= 0; rseed += 1) {
    print(rseed)
    printflush(message1)

    for (y = 0; y < dsize; y += 1) {
        for (x = 0; x < dsize; x += 1) {
            val = op(noise, x + rseed, y + rseed)
            val = op(abs, val, _)
            val *= 255
            draw.color(val, val, val, 255)
            draw.rect(x * PM, y * PM, PM, PM)
        }
    }
    drawflush(display1)
}
